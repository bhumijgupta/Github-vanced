{
    "manifest_version": 2,
    "name": "Github Contri",
    "description": "Don't get distracted by contribution chart on GitHub. Color it all green",
    "version": "1.0.0",
    "author": "Bhumij Gupta",
    "icons": {
        "16": "icon_16.png",
        "32": "icon_32.png",
        "48": "icon_48.png",
        "128": "icon_128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "http://github.com/*",
                "https://github.com/*"
            ],
            "js": [
                "colorify.js"
            ],
            "run_at": "document_end"
        }
    ]
}